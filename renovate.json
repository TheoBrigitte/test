{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        ".*y[a]?ml$"
      ],
      "matchStrings": [
        "name: (?<depName>.*)@(?<currentValue>.*)"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "giantswarm/{{{depName}}}"
    }
  ]
}
